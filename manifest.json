{
"update_url":"http://clients2.google.com/service/update2/crx",
  "name": "BA.net AdBlock"
  ,"version": "1.2"
  ,"manifest_version": 2
  ,"description": "BA.net AdBlock"
  ,"icons": { "128": "asset/image/tunnel_switch.png" }
  ,"background": {
    "page" : "asset/html/background.html"
  }
  ,"options_page": "asset/html/options.html"
  ,"permissions": [
    "proxy", "tabs", "http://*/*", "https://*/*" 
  ]
  ,"browser_action": {
      "default_title": "Click to toogle AdBlock"
      ,"default_icon":"asset/image/direct.png"
  }
  ,"homepage_url":"http://ba.net/adblock"
}
